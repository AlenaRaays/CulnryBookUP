@model IEnumerable<CulnryBookUP.Models.User>

<link rel="stylesheet" href="~/css/site.css" />

<h2>Clients</h2>

<table class="table-bordered">
    <thead>
        <tr>
            <th>IdAutor</th>
            <th>Login</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.UserName</td>
                <td>@item.Login</td>
                <td>@item.Password</td>
            </tr>
        }
    </tbody>
</table>
